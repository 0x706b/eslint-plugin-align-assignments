{
  "name": "@0x706b/eslint-plugin-align-assignments",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/0x706b/eslint-plugin-align-assignments"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc && node package.js",
    "commit": "cz",
    "link:local": "cd dist && yarn link && cd ../ && yarn link \"@0x706b/eslint-plugin-align-assignments\"",
    "unlink:local": "yarn unlink \"@0x706b/eslint-plugin-align-assignments\" && cd dist && yarn unlink && cd ../",
    "publish:package": "cd dist && yarn publish && cd ../"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/experimental-utils": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.6.0",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "typescript": "^4.5.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
